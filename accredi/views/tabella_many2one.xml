<odoo>

    <record id="test_model_2_action" model="ir.actions.act_window">
            <field name="name">Tabella2one</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">pratiche</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Crea un nuovo record ,muoviti
                </p>
            </field>
    </record>


    <record id="vista_tabellare_view_2" model="ir.ui.view">
        <field name="name">Vista_pratiche</field>
        <field name="model">pratiche</field>
        <field name="arch" type="xml">
            <tree>
                <field name="tipo_pratica" string="Tipologia Pratiche"/>
            </tree>
        </field>
    </record>

    <record id="form_tabella_view_2" model="ir.ui.view">
        <field name="name">Form_pratiche</field>
        <field name="model">pratiche</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="tipo_pratica"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>



</odoo>


